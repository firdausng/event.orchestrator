{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KafkaConsumer": {
    "AllowAutoCreateTopics": true,
    "BatchTimeoutInSec": 1
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8181"
      },
      "Prometheus": {
        "Url": "http://0.0.0.0:9191"
      }
    }
  },
  "Prometheus": {
    "LocalPort": 9191
  }
}
